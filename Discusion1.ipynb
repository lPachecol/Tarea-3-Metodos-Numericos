    "def suma_inversa(lista):\n",
    "    suma = 0\n",
    "    for x in reversed(lista):\n",
    "        suma += x\n",
    "    return suma\n",
    "\n",
    "# Ejemplo de uso\n",
    "numeros = [0.1, 0.01, 0.001, 0.0001]\n",
    "print(\"Suma directa:\", sum(numeros))\n",
    "print(\"Suma inversa:\", suma_inversa(numeros))"
   ]
  }
